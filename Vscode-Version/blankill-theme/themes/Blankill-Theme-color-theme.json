{
  "name": "blankill-theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1C1C1C",
    "editor.foreground": "#FFFFFF",
    "editor.selectionBackground": "#4e2f2f",
    "editor.lineHighlightBackground": "#856363",
    "editorCursor.foreground": "#d8b5ff",
    "sideBar.background": "#1C1C1C",
    "sideBar.foreground": "#FFFFFF",
    "sideBar.border": "#000000",

    "activityBar.background": "#101010",
    "activityBar.foreground": "#FFFFFF",
    "activityBar.activeBorder": "#FF5F24",
    "activityBar.inactiveForeground": "#777777",

    "list.inactiveSelectionBackground": "#2A2A2A",
    "list.activeSelectionBackground": "#3A3A3A",
    "list.hoverBackground": "#252525",
    "list.highlightForeground": "#FF5F24"

  },
  "tokenColors": [
    { "scope": "comment", "settings": { "foreground": "#497A47" } },
    { "scope": "string", "settings": { "foreground": "#E2CA76" } },
    { "scope": "constant.numeric", "settings": { "foreground": "#CCC0C0" } },
    { "scope": "keyword", "settings": { "foreground": "#ff2424" } },
    { "scope": "storage.type", "settings": { "foreground": "#FF0000" } },
    { "scope": "entity.name.type", "settings": { "foreground": "#fc3e58" } },
    { "scope": "support.type", "settings": { "foreground": "#fc3e58" } },
    { "scope": "entity.name.function", "settings": { "foreground": "#0DA3B5" } },
    { "scope": "storage.modifier", "settings": { "foreground": "#FF8300" } },
    { "scope": "keyword.operator", "settings": { "foreground": "#00D0FF" } },
    { "scope": "punctuation", "settings": { "foreground": "#FFFFFF" } },
    
    
  ]
}
