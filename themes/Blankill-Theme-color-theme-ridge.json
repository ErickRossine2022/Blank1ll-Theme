{
  "$schema": "vscode://schemas/color-theme",
  "name": "Blankill Theme Night Ridge",
  "type": "dark",

  "colors": {
    "editor.background": "#0B1C2D",
    "editor.foreground": "#DCE6ED",

    "editor.selectionBackground": "#1E3A52",
    "editor.lineHighlightBackground": "#102A3A",

    "editorCursor.foreground": "#6FAED9",

    "sideBar.background": "#081724",
    "sideBar.foreground": "#C9D6DF",
    "sideBar.border": "#000000",

    "activityBar.background": "#06121D",
    "activityBar.foreground": "#C9D6DF",
    "activityBar.inactiveForeground": "#6C7A86",
    "activityBar.activeBorder": "#6FAED9",

    "button.background": "#102A3A",
    "button.foreground": "#DCE6ED",
    "button.hoverBackground": "#1E3A52",

    "focusBorder": "#6FAED9",

    "progressBar.background": "#6FAED9",

    "badge.background": "#1E3A52",
    "badge.foreground": "#FFFFFF",

    "list.inactiveSelectionBackground": "#102A3A",
    "list.activeSelectionBackground": "#1E3A52",
    "list.hoverBackground": "#0F2434",
    "list.highlightForeground": "#6FAED9"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5F7D8C",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#FFD166",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "keyword",
      "settings": {
        "foreground": "#B38BFA"
      }
    },

    {
      "scope": [
        "storage.type",
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#A78BFA"
      }
    },

    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#6EC1E4"
      }
    },

    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#E0E6ED"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#8FD3E8"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#9FB3C8"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#9FB3C8"
      }
    },

    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#8A9BA8"
      }
    },
    {
      "scope": "punctuation.section.block",
      "settings": {
        "foreground": "#6FAED9"
      }
    },
    {
      "scope": "punctuation.section.parens",
      "settings": {
        "foreground": "#9FB3C8"
      }
    },

    {
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#6FAED9"
      }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#FFD166"
      }
    },

    {
      "scope": "entity.name.selector.css",
      "settings": {
        "foreground": "#B38BFA"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#E0E6ED"
      }
    },
    {
      "scope": [
        "constant.other.color.rgb-value.css",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#FFD166"
      }
    }
  ]
}
