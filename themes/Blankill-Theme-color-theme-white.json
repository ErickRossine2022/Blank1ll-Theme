{
  "$schema": "vscode://schemas/color-theme",
  "name": "Blankill Theme n√£o sei",
  "type": "light",

  "colors": {
  "editor.background": "#ebe2e2",
  "editor.foreground": "#1E1E1E",

  "editor.selectionBackground": "#D6D9FF",
  "editor.lineHighlightBackground": "#ebe2e2",

  "editorCursor.foreground": "#6C63FF",

  "sideBar.background": "#ebe2e2",
  "sideBar.foreground": "#111111",
  "sideBar.border": "#000000",

  "activityBar.background": "#ac9393",
  "activityBar.foreground": "#1E1E1E",
  "activityBar.inactiveForeground": "#6E6E6E",
  "activityBar.activeBorder": "#6C63FF",

  "button.background": "#DDDDDD",
  "button.foreground": "#1E1E1E",
  "button.hoverBackground": "#D0D0D0",

  "focusBorder": "#6C63FF",
  "progressBar.background": "#6C63FF",

  "badge.background": "#DDDDDD",
  "badge.foreground": "#1E1E1E",

  "list.activeSelectionForeground": "#000000",
  "list.inactiveSelectionForeground": "#111111",
  "list.hoverForeground": "#000000",

  "list.inactiveSelectionBackground": "#E2E2E2",
  "list.activeSelectionBackground": "#D6D6D6",
  "list.hoverBackground": "#EAEAEA",
  "list.highlightForeground": "#6C63FF",

  "icon.foreground": "#111111"
},

"tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#3F6F4F",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#8A4A1A"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#2B2B2B"
      }
    },

    {
      "scope": [
        "keyword",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#5A1E32",
        "fontStyle": "bold"
      }
    },

    {
      "scope": [
        "storage.type",
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#4E46B4"
      }
    },

    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#0F4F7A"
      }
    },

    {
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#125E94"
      }
    },

    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "bold"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#3A3A3A"
      }
    },

    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#1E1E1E"
      }
    },

    {
      "scope": [
        "punctuation.section.block",
        "punctuation.section.parens"
      ],
      "settings": {
        "foreground": "#4E46B4"
      }
    },

    {
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#4E46B4"
      }
    },

    {
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#5A1E32"
      }
    },

    {
      "scope": "entity.name.selector.css",
      "settings": {
        "foreground": "#4E46B4"
      }
    },

    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#1E1E1E"
      }
    },

    {
      "scope": [
        "constant.other.color.rgb-value.css",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#8A4A1A"
      }
    }
  ]

}
