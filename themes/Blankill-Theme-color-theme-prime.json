{
  "$schema": "vscode://schemas/color-theme",
  "name": "Blankill Theme Prime",
  "type": "dark",
  "colors": {
    "editor.background": "#181818",
    "editor.foreground": "#EDEDED",

    "editor.selectionBackground": "#3A2E4F",
    "editor.lineHighlightBackground": "#242424",

    "editorCursor.foreground": "#C792EA",

    "sideBar.background": "#141414",
    "sideBar.foreground": "#EDEDED",
    "sideBar.border": "#000000",

    "activityBar.background": "#101010",
    "activityBar.foreground": "#EDEDED",
    "activityBar.activeBorder": "#FF6A2A",
    "activityBar.inactiveForeground": "#777777",

    "button.background": "#2C2C2C",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#FF6A2A",

    "focusBorder": "#FF6A2A",

    "progressBar.background": "#FF6A2A",

    "badge.background": "#FF6A2A",
    "badge.foreground": "#FFFFFF",

    "list.inactiveSelectionBackground": "#262626",
    "list.activeSelectionBackground": "#333333",
    "list.hoverBackground": "#202020",
    "list.highlightForeground": "#FF6A2A"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": { "foreground": "#497A47", "fontStyle": "italic" }
    },

    {
      "scope": "string",
      "settings": { "foreground": "#E2CA76" }
    },

    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#CCC0C0" }
    },

    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.flow"
      ],
      "settings": { "foreground": "#FF2424" }
    },

    {
      "scope": [
        "storage.type",
        "storage.type.class",
        "storage.type.interface"
      ],
      "settings": { "foreground": "#FF0000" }
    },

    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "entity.name.class"
      ],
      "settings": { "foreground": "#FC3E58" }
    },

    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#0DA3B5" }
    },

    {
      "scope": [
        "storage.modifier",
        "keyword.modifier"
      ],
      "settings": { "foreground": "#FF8300" }
    },

    {
      "scope": [
        "keyword.operator",
        "punctuation.separator"
      ],
      "settings": { "foreground": "#00D0FF" }
    },

    {
      "scope": "punctuation",
      "settings": { "foreground": "#FFFFFF" }
    },

    {
      "scope": "entity.name.tag.html",
      "settings": { "foreground": "#FF5F24" }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "settings": { "foreground": "#E2CA76" }
    },
    {
      "scope": "string.quoted.double.html",
      "settings": { "foreground": "#CCC0C0" }
    },

    {
      "scope": "entity.name.selector.css",
      "settings": { "foreground": "#FC3E58" }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#FF8300" }
    },
    {
      "scope": "constant.other.color.rgb-value.css",
      "settings": { "foreground": "#00D0FF" }
    },
    {
      "scope": "constant.numeric.css",
      "settings": { "foreground": "#CCC0C0" }
    },

    {
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other.readwrite"
      ],
      "settings": { "foreground": "#FFFFFF" }
    }
  ]
}
