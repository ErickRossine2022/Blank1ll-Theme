{
  "$schema": "vscode://schemas/color-theme",
  "name": "Blankill Theme Black",
  "type": "dark",

  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#FFFFFF",

    "editor.selectionBackground": "#2A203A",
    "editor.lineHighlightBackground": "#111111",

    "editorCursor.foreground": "#B38BFA",

    "sideBar.background": "#0A0A0A",
    "sideBar.foreground": "#CCCCCC",
    "sideBar.border": "#000000",

    "activityBar.background": "#0A0A0A",
    "activityBar.foreground": "#BBBBBB",
    "activityBar.inactiveForeground": "#666666",
    "activityBar.activeBorder": "#FF6A2A",

    "button.background": "#1A1A1A",
    "button.foreground": "#DDDDDD",
    "button.hoverBackground": "#FF6A2A",

    "focusBorder": "#FF6A2A",

    "progressBar.background": "#FF6A2A",

    "badge.background": "#FF6A2A",
    "badge.foreground": "#FFFFFF",

    "list.inactiveSelectionBackground": "#111111",
    "list.activeSelectionBackground": "#1A1A1A",
    "list.hoverBackground": "#141414",
    "list.highlightForeground": "#FF6A2A"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5F875F",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#E6D7A3"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },

    {
      "scope": "keyword",
      "settings": {
        "foreground": "#9b66fd"
      }
    },

    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#A98AF7"
      }
    },

    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#B38BFA"
      }
    },

    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7FE9FF"
      }
    },

    {
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#6CCEFF"
      }
    },

    {
      "scope": "support.class",
      "settings": {
        "foreground": "#B38BFA"
      }
    },

    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#ff6c6c"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#CFCFCF"
      }
    },

    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },

    {
      "scope": "punctuation.section.block",
      "settings": {
        "foreground": "#7A6AFF"
      }
    },

    {
      "scope": "punctuation.section.parens",
      "settings": {
        "foreground": "#AAAAAA"
      }
    },

    {
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#9A7AFF"
      }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },

    {
      "scope": "entity.name.selector.css",
      "settings": {
        "foreground": "#B38BFA"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": "constant.other.color.rgb-value.css",
      "settings": {
        "foreground": "#9A7AFF"
      }
    },
    {
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#9A7AFF"
      }
    }
  ]
}
