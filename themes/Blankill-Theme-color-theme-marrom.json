{
  "$schema": "vscode://schemas/color-theme",
  "name": "Blankill Theme Marrom",
  "type": "dark",

  "colors": {
    "editor.background": "#181818",
    "editor.foreground": "#EDEDED",

    "editor.selectionBackground": "#3A2E4F",
    "editor.lineHighlightBackground": "#242424",

    "editorCursor.foreground": "#C792EA",

    "sideBar.background": "#141414",
    "sideBar.foreground": "#EDEDED",
    "sideBar.border": "#000000",

    "activityBar.background": "#101010",
    "activityBar.foreground": "#EDEDED",
    "activityBar.activeBorder": "#FF6A2A",
    "activityBar.inactiveForeground": "#777777",

    "button.background": "#2C2C2C",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#FF6A2A",

    "focusBorder": "#FF6A2A",

    "progressBar.background": "#FF6A2A",

    "badge.background": "#FF6A2A",
    "badge.foreground": "#FFFFFF",

    "list.inactiveSelectionBackground": "#262626",
    "list.activeSelectionBackground": "#333333",
    "list.hoverBackground": "#202020",
    "list.highlightForeground": "#FF6A2A"
  },

  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#1D6719",
        "fontStyle": "italic"
      }
    },

    {
      "scope": "string",
      "settings": {
        "foreground": "#9C4317"
      }
    },

    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#CCC0C0"
      }
    },

    {
      "scope": "keyword",
      "settings": {
        "foreground": "#9C1717"
      }
    },

    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#B07A5A"
      }
    },

    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#B07A5A"
      }
    },

    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#177696"
      }
    },

    {
      "scope": "meta.function-call",
      "settings": {
        "foreground": "#2C8FB3"
      }
    },

    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#EDEDED"
      }
    },

    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#D0B49F"
      }
    },

    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#EDEDED"
      }
    },

    {
      "scope": "punctuation.section.block",
      "settings": {
        "foreground": "#8F5E3B"
      }
    },

    {
      "scope": "punctuation.section.parens",
      "settings": {
        "foreground": "#BFBFBF"
      }
    },
    
    {
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#8F5E3B"
      }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#D0B49F"
      }
    },

    {
      "scope": "entity.name.selector.css",
      "settings": {
        "foreground": "#B07A5A"
      }
    },
    {
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#EDEDED"
      }
    },
    {
      "scope": "constant.other.color.rgb-value.css",
      "settings": {
        "foreground": "#9C4317"
      }
    },
    {
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#9C4317"
      }
    }
  ]
}
